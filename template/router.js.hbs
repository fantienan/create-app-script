// 路由配置
export interface Routers {
  key: string
  name?: string
  to?: string
  path: string
  component: string | (() => any)
  exact?: boolean
  wrappers?: string[]
  redirect?: string
  routes?: any[]
  [k: string]: any
}

export const routers: Routers[] = [
  /* @create-app-script-split-start*/
  {{#each list}}
  { 
    key: '{{key}}',
    name: '{{name}}',
    to: '{{to}}',
    path: '{{path}}',
    component: '{{component}}',
    exact: true,
    {{#if popover}}
    popover: {{popover}},
    {{/if}}
    {{#if routes.length}}
    routes: [
    {{#each routes}}
      { 
        key: '{{key}}',
        name: '{{name}}',
        to: '{{to}}',
        path: '{{path}}',
        component: '{{component}}',
        exact: true,
        {{#if popover}}
        popover: {{popover}}
        {{/if}}
      },
    {{/each}}
    ]
    {{/if}}
  },
  {{/each}}
  /* @create-app-script-split-end*/
]